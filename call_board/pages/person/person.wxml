<view class="individual">

  <view class="top">

    <view class="user" style='display:flex'>
      <open-data type="userAvatarUrl" class="avatar"></open-data>
      <open-data type="userNickName" class="name"></open-data>
    </view>
  </view>

  <view class="menu">
    <view class='item about' bindtap='tomyinfo'>
      <text class='sub-title'>个人信息</text>
    </view>

    <view class="item about" bindtap="tomyissue">
      <text class="sub-title">我的发布</text>
    </view>

    <view class="item about" bindtap="tocollect">
      <text class="sub-title">我的收藏</text>
    </view>

    <view class="item about" bindtap="showabout">
      <text class="sub-title">关于</text>
    </view>

    <view class="item complain">
      <navigator target="miniProgram" open-type="navigate" app-id="wx8abaf00ee8c3202e" extra-data="{{ extraData }}" version="0.9.3">
        <text class='sub-title'>我要吐槽</text>
      </navigator>
    </view>

    <view class="item about" bindtap="tohelp">
      <text class="sub-title">使用帮助</text>
    </view>
  </view>

  <view class='menu'>
    <view class='item about' bindtap='toadmin'>
      <text class='sub-title'>管理员审核</text>
    </view>
  </view>

</view>
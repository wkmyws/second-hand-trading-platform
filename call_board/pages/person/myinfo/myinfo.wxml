<view class='content'>

  <view class='top'>

    <view class='nickpic'>
      <open-data type="userAvatarUrl"></open-data>
    </view>

    <view class='nickname'>
      <open-data type="userNickName"></open-data>
    </view>

  </view>

  <view class='menu'>
    <view class='item'>
      <text class='left'>姓名：{{my_info.user_name}}</text>
    </view>

    <view class='item'>
      <text class='left'>性别：{{my_info.user_sex}}</text>
    </view>


    <view class='item'>
      <text class='left'>学号：</text>
    </view>

    <view class='item'>
      <text class='left'>书院：</text>
    </view>

    <view class='item'>
      <text class='left'>微信号：</text>
    </view>

    <view class='item'>
      <text class='left'>QQ号：</text>
    </view>

    <view class='item'>
      <text class='left'>电话号码：</text>
    </view>
  </view>



  <button class='butt' bindtap='ToEdit'>编辑信息</button>

</view>
<view class='content'>

  <view class='top'>

    <view class='nickpic'>
      <open-data type="userAvatarUrl"></open-data>
    </view>

    <view class='nickname'>
      <view class="name">{{my_info.user_name}}</view>
      <!--<open-data type="userNickName" ></open-data>-->
    </view>

  </view>

  <view class='menu'>
      <view class='item'>
      <text class='left'>UID：</text>
      <text class='right'wx:if="{{my_info.user_id!=null}}">{{my_info.user_id}}</text>
    </view>

    <view class='item'>
      <text class='left'>昵称：</text>
      <text class='right'wx:if="{{my_info.user_name!=null}}">{{my_info.user_name}}</text>
    </view>

    <view class='item'>
      <text class='left'>性别：</text>
      <text class='right' wx:if="{{my_info.user_sex!=null}}">{{my_info.user_sex}}</text>
    </view>

    <view class='item'>
      <text class='left'>学校：</text>
      <text class='right' wx:if="{{my_info.user_school_name!=null}}">{{my_info.user_school_name}}</text>
    </view>

    <view class='item'>
      <text class='left' >微信号：</text>
      <text class='right' wx:if="{{my_info.user_wechat!=null}}">{{my_info.user_wechat}}</text>
    </view>

    <view class='item'>
      <text class='left'>QQ号：</text>
      <text class='right' wx:if="{{my_info.user_qq!=null}}">{{my_info.user_qq}}</text>
    </view>

    <view class='item'>
      <text class='left'>电话号码：</text>
      <text class='right' wx:if="{{my_info.user_phone!=null}}">{{my_info.user_phone}}</text>
    </view>
  </view>



  <button class='butt' bindtap='ToEdit'>编辑信息</button>

</view>